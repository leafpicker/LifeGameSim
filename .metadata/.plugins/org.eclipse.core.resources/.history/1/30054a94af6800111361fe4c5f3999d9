package ca.sfu.cmpt431.facility;

public class Outfits {
	
	public int nextClock;
	public int top;
	public int left;
	public Board myBoard;
	public Comrade [] neighbours;
	public Comrade pair;
	
	public Outfits(int clk, int t, int l, int h, int w) {
		myBoard = new Board(h, w);
		neighbours = new Comrade();
	}
	
}
